+        int addr = (lookup >> 16) & 0xFFFF;
+        int data = lookup & 0xFFFF;
+        if (addr == 0xFFFE) {
+            delay_ms(data);
+        } else if (addr == 0xFFFF) {
+            return;
+        } else {
