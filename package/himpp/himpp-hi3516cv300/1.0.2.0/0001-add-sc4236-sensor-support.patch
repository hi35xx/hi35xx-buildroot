+HI_VOID cmos_set_pixel_detect(HI_BOOL bEnable)
+{
+    printf("cmos_set_pixel_detect\n");
+    HI_U32 u32FullLines_5Fps = VMAX_1536P15_LINEAR * 15 / 5;
+    u32FullLines_5Fps = (u32FullLines_5Fps > FULL_LINES_MAX) ? FULL_LINES_MAX : u32FullLines_5Fps;
+    HI_U32 u32MaxExpTime_5Fps = u32FullLines_5Fps * 2 - 4;
+
+    if (bEnable) /* setup for ISP pixel calibration mode */
+    {
+        sensor_write_register(0x320E, (u32FullLines_5Fps & 0xFF00) >> 8);    /* 5fps */
+        sensor_write_register(0x320F, (u32FullLines_5Fps & 0xFF));             /* 5fps */
+        sensor_write_register(0x3E00, ((u32MaxExpTime_5Fps >> 12) & 0x0F));  /* max exposure lines */
+        sensor_write_register(0x3E01, ((u32MaxExpTime_5Fps >> 4) & 0xFF));   /* max exposure lines */
+        sensor_write_register(0x3E02, ((u32MaxExpTime_5Fps << 4) & 0xF0));   /* max exposure lines */
+        sensor_write_register(0x3E06, 0x00);                                 /* min Gain = 1*/
+        sensor_write_register(0x3E07, 0x80);
+        sensor_write_register(0x3E08, 0x03);
+        sensor_write_register(0x3E09, 0x10);
+    }
+    else /* setup for ISP 'normal mode' */
+    {
+        gu32FullLinesStd = (gu32FullLinesStd > FULL_LINES_MAX) ? FULL_LINES_MAX : gu32FullLinesStd;
+        gu32FullLines = gu32FullLinesStd;
+        sensor_write_register(0x320E, ((gu32FullLinesStd & 0xFF00) >> 8));  /* Standard FPS */
+        sensor_write_register(0x320F, (gu32FullLinesStd & 0xFF));           /* Standard FPS */
+        bInit = HI_FALSE;
+    }
+
+    return;
+}
