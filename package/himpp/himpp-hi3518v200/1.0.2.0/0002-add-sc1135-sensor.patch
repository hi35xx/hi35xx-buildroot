+int sensor_read_register(int addr)
+{	 
+	int idx = 0;
+	int ret = 0, data = 0;
+	char buf[8] = {0};
+	buf[idx++] = addr & 0xFF;
+
+	if (sensor_addr_byte == 2)
+	{
+		ret = ioctl(g_fd, I2C_16BIT_REG, 1);
+		buf[idx++] = (addr >> 8);
+	}
+	else
+	{
+		ret = ioctl(g_fd, I2C_16BIT_REG, 0);
+	}
+	if (ret < 0)
+	{
+		printf("CMD_SET_REG_WIDTH error!\n");
+		return -1;
+	}
+
+	buf[idx++] = data;
+	if (sensor_data_byte == 2)
+	{
+		ret = ioctl(g_fd, I2C_16BIT_DATA, 1);
+		buf[idx++] = data >> 8;
+	}
+	else
+	{
+		ret = ioctl(g_fd, I2C_16BIT_DATA, 0);
+	}
+	if (ret)
+	{
+		printf("hi_i2c write faild!\n");
+		return -1;
+	}
+	ret = read(g_fd, buf, idx);
+
+	return buf[0];
+}
+
+
+
+
+int sensor_write_register(int addr, int data)
+{
+#ifdef HI_GPIO_I2C
+	i2c_data.dev_addr = sensor_i2c_addr;
+	i2c_data.reg_addr = addr;
+	i2c_data.addr_byte_num = sensor_addr_byte;
+	i2c_data.data = data;
+	i2c_data.data_byte_num = sensor_data_byte;
+
+	ret = ioctl(g_fd, GPIO_I2C_WRITE, &i2c_data);
+
+	if (ret)
+	{
+		printf("GPIO-I2C write faild!\n");
+		return ret;
+	}
+#else
+	if(flag_init == 0)
+	{
+
+		sensor_i2c_init();
+		flag_init = 1;
+	}
+
+	int idx = 0;
+	int ret;
+	char buf[8];
+
+	buf[idx++] = addr & 0xFF;
+	if (sensor_addr_byte == 2)
+	{
+		ret = ioctl(g_fd, I2C_16BIT_REG, 1);
+		buf[idx++] = addr >> 8;
+	}
+	else
+	{
+		ret = ioctl(g_fd, I2C_16BIT_REG, 0);
+	}
+
+	if (ret < 0)
+	{
+		printf("CMD_SET_REG_WIDTH error!\n");
+		return -1;
+	}
+
+	buf[idx++] = data;
+	if (sensor_data_byte == 2)
+	{
+		ret = ioctl(g_fd, I2C_16BIT_DATA, 1);
+		buf[idx++] = data >> 8;
+	}
+	else
+	{
+		ret = ioctl(g_fd, I2C_16BIT_DATA, 0);
+	}
+
+	if (ret)
+	{
+		printf("hi_i2c write faild!\n");
+		return -1;
+	}
+
+	ret = write(g_fd, buf, idx);
+	if(ret < 0)
+	{
+		printf("I2C_WRITE error!\n");
+		return -1;
+	}
+#endif
+
+	return 0;
+}
