ifeq ($(CONFIG_OSD_ENABLE), y)

ifeq ($(CONFIG_PRODUCTNAME), "godnet")
LIBS += ./product/hiosd/vo/godnet/libvo.a
LIBS += ./product/hiosd/hdmi/godnet/libhdmi.a
LIBS += ./product/hiosd/dec/libjpegd.a
endif

ifeq ($(CONFIG_PRODUCTNAME), "godarm")
LIBS += ./product/hiosd/vo/godarm/libvo.a
LIBS += ./product/hiosd/hdmi/godarm/libhdmi.a
LIBS += ./product/hiosd/dec/libjpegd.a
endif

ifeq ($(CONFIG_PRODUCTNAME), "hi3535")
LIBS += ./product/hiosd/vo/hi3535/libvo.a
LIBS += ./product/hiosd/hdmi/hi3535/libhdmi.a
LIBS += ./product/hiosd/dec/libjpegd.a
endif
ifeq ($(CONFIG_PRODUCTNAME), "hi3520d")
LIBS += ./product/hiosd/vo/hi3520d/libvo.a
LIBS += ./product/hiosd/hdmi/hi3520d/libhdmi.a
LIBS += ./product/hiosd/dec/libjpegd.a
endif
endif
